{
  "name": "vite-plugin-strip-block",
  "version": "1.0.0",
  "description": "vite build code block strip plugin",
  "homepage": "https://github.com/gseok/vite-plugin-strip-block",
  "repository": {
    "type": "git",
    "url": "https://github.com/gseok/vite-plugin-strip-block"
  },
  "keywords": [
    "strip",
    "vite",
    "vite-plugin"
