  "scripts": {
    "build": "tsup src/index.ts --dts --minify --format cjs,esm",
    "deploy": "pnpm publish"
  },
  "devDependencies": {
    "tsup": "^6.7.0",
    "typescript": "^5.1.3",
    "vite": "^4.3.9"
